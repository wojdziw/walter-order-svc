{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended", "tslint-config-airbnb-base"
    ],
    "typeCheck": true,
    "jsRules": {
    },
    "rules": {
        "interface-name": [true, "never-prefix"],
        "ter-arrow-parens": [true, "always"],
        "arrow-parens": false,
        "no-consecutive-blank-lines": true,
        "no-magic-numbers": [true, -1, 0, 1, 10],
        "no-console": [true, "log"]
    },
    "rulesDirectory": []
}